{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build_electron": "vue-cli-service build --dest dist_electron",
    "test_unit_datalocal": "mocha --compilers js:babel-core/register --timeout 20000 test_unit/test_dataLocal",
    "test_unit_base": "mocha --timeout 20000 test_unit/test_unit_base",
    "test_ui_base": "mocha --timeout 20000 test_ui/test_ui_base",
    "test": "mocha --timeout 20000 test_unit/"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vue-sweet-calendar": "^0.3.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "chai": "^4.2.0",
    "puppeteer": "^1.19.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "nodemonConfig": {
    "ignore": [
      "test_ui/*",
      "test_unit/*"
    ],
    "delay": "2500"
  }
}
